@model RegesterVM
@{
    Layout = "~/Views/Shared/_UserLayout.cshtml";
}
<p class="lead fw-normal mb-0 me-3"> Regestir</p>
<div class="container-fluid h-custom">
    <div class="row d-flex justify-content-center align-items-center h-100">
        <form asp-action="Register">
    <div asp-validation-summary="All"></div>

    <label asp-for="Fname" class="form-label"></label>
    <input asp-for="Fname" class="form-control form-control-lg" />
    <span asp-validation-for="Fname" class="text-danger"></span>

    <label asp-for="Lname" class="form-label"></label>
    <input asp-for="Lname" class="form-control form-control-lg" />
    <span asp-validation-for="Lname" class="text-danger"></span>

    <label asp-for="Gender" class="form-label"></label>
        <input asp-for="Gender" class="form-control form-control-lg" />
    <span asp-validation-for="Gender" class="text-danger"></span>
    <br />
    <label asp-for="Email" class="form-label"></label>
        <input asp-for="Email" class="form-control form-control-lg" id="Datestart" />
    <span asp-validation-for="Email" class="text-danger"></span>

    <br />
            <label asp-for="EmailConfirmed" class="form-label"></label>
            <input asp-for="EmailConfirmed" class="form-control form-control-lg" id="Datestart" />
            <span asp-validation-for="EmailConfirmed" class="text-danger"></span>

            <br />
    <label asp-for="Password" class="form-label"></label>
        <input asp-for="Password" class="form-control form-control-lg" id="Datestart" />
    <span asp-validation-for="Password" class="text-danger"></span>
    <br />
    <label asp-for="ConfirmPassword" class="form-label"></label>
        <input asp-for="ConfirmPassword" class="form-control form-control-lg" id="Datestart" />
    <span asp-validation-for="ConfirmPassword" class="text-danger"></span>
    <br />

      <input type="submit" class="btn btn-primary btn-lg m-2" />
     <a asp-action="Index" class="btn btn-dark"> Back to List</a>
</form>
</div>
</div>